  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Long" >
    select 
    <include refid="Base_Column_List" />
    from tb_rescue
    where rescue_id = #{rescueId,jdbcType=BIGINT}
    and delete_flag =0
  </select>

  <select id="findRescue" resultType="com.ddcar.entity.UserRescue" parameterType="Map">
    select rescue_address, rescue_time, r.user_id, u.user_name, u.phone, u.rescue, u.head_photo,
    r.status, r.rescue_id from tb_rescue r join tb_user u on r.user_id = u.user_id
    where r.wang_user_id = #{wangUserId} and r.user_id = #{userId}
    and (r.status = 1 or r.status = 2) and r.delete_flag =0
  </select>
  <!--查询救援信息-->
  <select id="rescueList" resultType="java.util.HashMap">
    SELECT rescue_id AS rescueId,rescue_address AS rescueAddress,rescue_time AS rescueTime,
    IFNULL((SELECT user_name FROM tb_user WHERE delete_flag=0 AND user_id=t1.`user_id`),'') AS userName,
    IFNULL((SELECT phone FROM tb_user WHERE delete_flag=0 AND user_id=t1.`user_id`),'') AS userphone,
    IFNULL((SELECT branch_name FROM tb_branch WHERE delete_flag=0 AND branch_id=t1.`wang_user_id`),'') AS branchName,
    IFNULL((SELECT branch_phone FROM tb_branch WHERE delete_flag=0 AND branch_id=t1.`wang_user_id`),'') AS branchPhone,
    IFNULL((SELECT branchaaddr FROM tb_branch WHERE delete_flag=0 AND branch_id=t1.`wang_user_id`),'') AS branchaaddr
    FROM tb_rescue t1 WHERE t1.`delete_flag`=0
    AND EXISTS ( SELECT branch_id FROM tb_branch WHERE delete_flag=0 AND branch_id=t1.`wang_user_id` AND parent_id=#{branchId})
    <if test="userName != null and userName!=''">
    AND EXISTS (SELECT 1 FROM tb_user WHERE delete_flag=0 AND user_id=t1.`user_id` AND user_name LIKE '%${userName}%')
    </if>
    <if test="phone != null and phone!=''">
   AND EXISTS (SELECT 1 FROM tb_user WHERE delete_flag=0 AND user_id=t1.`user_id` AND phone LIKE '%${phone}%')
    </if>
    ORDER BY rescue_id DESC
  </select>
  <select id="findRescuexin" resultType="com.ddcar.entity.RescueXin" parameterType="long">
    select r.rescue_address, r.create_time, r.status,r.rescue_id,r.modify_time,
    b.branch_phone, b.branchaaddr, b.branch_name,r.repair_name,r.repair_phone
    from tb_rescue r join tb_branch b on r.wang_user_id = b.branch_id
    and user_id = #{userId,jdbcType=BIGINT} and (status = 1 or status = 2 or status = 3)
    and r.delete_flag = 0
  </select>

