    function allexal() {
        brandId=$('#searchbrandId').val();
        modelType=$('#searchmodelType').val();
        branchId=$('#searchbranchId').val();
        branchName=$('#searchbranchName').val();
        vehicleId=$('#searchvehicleId').val();
        state=$('#searchstate').val();
        var pram="brandId="+brandId+"&modelType="+modelType+"&branchId="+branchId+"&branchName="+branchName+"&vehicleId="+vehicleId+"&state="+state;
        location=ctx+'/vehicle/allTve?'+pram;
    }
    function res(qrCode){
        $('#divOne').empty();
        var qrcode= $('#divOne').qrcode(qrCode).hide();
        var canvas=qrcode.find('canvas').get(0);
        $('#imgOne').attr('src',canvas.toDataURL('image/jpg'));
        $("#chakanModal").modal("show");
    };
</script>


