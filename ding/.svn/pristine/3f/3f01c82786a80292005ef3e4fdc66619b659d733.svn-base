</div>
<div class="modal fade" id="chakanModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">二维码</h4>
            </div>
            <br>
            <div align="center">
            <div id="divOne"></div>
            <img id='imgOne' />
            </div>
            <br>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
            </div>
        </div>
    </div>
</div>
<footer th:replace="comm :: footer_web"></footer>
<script th:src="@{/js/jquery.qrcode.min.js}"></script>
</body>
</html>
<script type="text/javascript">
    var index ='';
    var rowData = [];
    var roleArray = [ "管理员", "普通用户" ];
    $(function() {
        //$(".tbsalelist").width(2200);
        //$(".tbsalelist").css("max-width", 2200);
        //initUpload('uploadBtn','showImg',null,1024);

        loadData(1);
        selectbrand();
        $('.keyupclick').on('keypress', function(evt){
            if (evt.keyCode == 13) {
                loadData(1);
            }

        });

    });
    function clearSearch(){
        $('#searchForm input').val('');
        $('#searchForm select').val('');
        $('#selectSalerSearch').val('');
        $('#selectKefuSearch').val('');
    }

    function showSystemLog(msgType,userId){
        parent.locationCurrMenu();
        parent.addPage("web登陆日志","${ctx}/pages/system/systemlog.jsp?msgType="+msgType+"&userId="+userId);

    }
    function refreshData(){
        loadData(pageinfo["page"]);
    }

    function loadData(page, size) {
        if (size != undefined) {
            pageinfo["size"] = size;
            $('#pagesize').html(size);
        }
        if (page == -1)
            pageinfo["page"]--;
        else if (page == -2)
            pageinfo["page"]++;
        else {
            if (page >= pageinfo["tp"] && pageinfo["tp"] > 0)
                pageinfo["page"] = pageinfo["tp"];
            else
                pageinfo["page"] = page;
        }
        pageinfo["brandId"]=$('#searchbrandId').val();
        pageinfo["modelType"]=$('#searchmodelType').val();
        pageinfo["branchId"]=$('#searchbranchId').val();
        pageinfo["branchName"]=$('#searchbranchName').val();
        pageinfo["vehicleId"]=$('#searchvehicleId').val();
        pageinfo["state"]=$('#searchstate').val();

        $('#table_content').html('<tr><td colspan="23">'+$('#divLoading').html()+'</td></tr>');
        httpPost(ctx+'/vehicle/findTbVehicleList',
            pageinfo,
            function(msg, status) {
                if (msg.status == 200) {
                    rowData = msg.rows;
                    var array = [];
                    for ( var i = 0; i < msg.rows.length; i++) {
                        var data = msg.rows[i];
                        array.push('<tr>');

                        array.push('<td><input  type="checkbox" class="i-checks" name="checkbox-tr" value="' + data["vehicleId"]+ '"/></td>');
                        array.push('<td >'+data["vehicleId"]+'</td>');
                        array.push('<td >'+data["modelNumber"].tbBrand.brandName+'</td>');
                        array.push('<td >'+data["modelNumber"].model+'</td>');
                        if (data["modelNumber"].modelType == 0) {
                            array.push('<td>车辆</td>');
                        } else if (data["modelNumber"].modelType == 1) {
                            array.push('<td>电池</td>');
                        }else{
                            array.push('<td>数据不匹配</td>');
                        }
                        if (data["state"] == 0) {
                            array.push('<td>可租</td>');
                        } else if (data["state"] == 1) {
                            array.push('<td>已租</td>');
                        }else if (data["state"] == 2) {
                            array.push('<td>维修</td>');
                        }else if (data["state"] == 3) {
                            array.push('<td>停用</td>');
                        }else{
                            array.push('<td>数据不匹配</td>');
                        }
                        array.push('<td >'+data["imei"]+'</td>');
                        array.push('<td >'+data["branchId"]+'</td>');
                        array.push('<td >'+data["branch"].branchName+'</td>');
                        array.push('<td >'+'<a href="../carBattery/updateTbVehicle?vehicleId='+data["vehicleId"]+'">编辑</a>'+'</td>');
                        array.push('<td >'+'<a onclick="res('+'&quot;'+data["qrCode"]+'&quot;'+')">查看</a>'+'</td>');
                        array.push('</tr>');
                    }
                    $('#table_content').html(array.join(''));
                    initpageInfo(msg.total);
                    array=[];

                } else {
                    alert(msg.msg);

                }
            });
    }
    function selectbrand() {
        httpPost(ctx+'/brand/findList',
            pageinfo,
            function(msg, status) {
                if (msg.status == 200) {
                    rowData = msg.rows;
                    var array = [];
                    for ( var i = 0; i < msg.rows.length; i++) {
                        var data = msg.rows[i];
                        var option='<option value="'+data.brandId+'">'+data.brandName+'</option>';
                        $("[name=selectbrand]").append(option);
                    }
                } else {
                    alert(msg.msg);
                }
            });
    }
    //批量删除
    function deleteData() {

        var delArr = [];
        $('input[name="checkbox-tr"]').each(function(index, data) {
            if($(this).parent().attr('class').indexOf('checked')!=-1){
                delArr.push($(this).val());
            }
        });
        if(delArr.length==0){
            swal({
                title:'信息提示',
                text:'请至少选择一项待删除！',
                type:'warning',
                confirmButtonText:'确定'
            });
            return;
        }
        swal({
            title: "请删除所选车辆吗？",
            text: "",
            type: "warning",
            showCancelButton: true,
            confirmButtonColor: "#DD6B55",
            confirmButtonText: "确定",
            cancelButtonText: "取消",
            closeOnConfirm: true
        },function(){

            var params="delArr="+delArr;
            $.post(ctx+"/vehicle/delTbVehicle",params,function(data){
                if(data["status"]==200){
                    swal({
                        title:"信息提示",
                        text:"删除成功",
                        type:"success",
                        confirmButtonText:'确定'
                    })
                    loadData(pageinfo["page"]);
                }else {
                    swal({
                        title:"信息提示",
                        text:"删除失败",
                        type:"error",
                        confirmButtonText:'确定'
                    })
                }
            });

        })
    }



