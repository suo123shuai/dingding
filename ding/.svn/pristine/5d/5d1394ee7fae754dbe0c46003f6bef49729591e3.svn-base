  <!--查询该公司型号的价格 -->
  <select id="modelprice" resultType="java.util.HashMap">
    SELECT price_id AS priceId,IFNULL(monthly_rent,'') AS monthlyRent,IFNULL(cash_deposit,'') AS cashDeposit,
  IFNULL((SELECT model FROM tb_model_number WHERE delete_flag=0 AND model_id=t1.`model_id`),'') AS modelName,
  IFNULL((SELECT model_type FROM tb_model_number WHERE delete_flag=0 AND model_id=t1.`model_id`),'') AS modelType,
  IFNULL(( SELECT brand_name FROM tb_brand WHERE delete_flag=0 AND brand_id=(SELECT brand_id FROM tb_model_number WHERE delete_flag=0 AND model_id=t1.`model_id`)),'') AS brandName
  FROM tb_price t1 WHERE delete_flag=0 AND branch_id=#{branchId}
  <if test="modelId != null and modelId>-1">
  AND EXISTS (SELECT 1 FROM tb_model_number WHERE delete_flag=0 AND model_id=t1.`model_id` AND t1.model_id=#{modelId})
  </if>
  <if test="modelType != null and modelType>-1">
  AND EXISTS (SELECT 1 FROM tb_model_number WHERE delete_flag=0 AND model_id=t1.`model_id` AND model_type=#{modelType})
  </if>
  <if test="brandId != null and brandId>-1">
  AND EXISTS (SELECT 1 FROM tb_model_number WHERE delete_flag=0 AND model_id=t1.`model_id` AND brand_id=#{brandId})
  </if>
    ORDER BY price_id DESC
  </select>
