#pager{
	font-size:12px;
}

#pager ul li{
	text-align:center;
	float:left;
	list-style:none;
	display:inline-block;
	margin-left:5px;
}
#pager ul li a{
	display:inline-block;
	border:#ccc 1px solid;
	cursor:pointer;
	text-decoration: none;
	padding:5px 10px;
}
#pager ul li a:hover{
	color:#fff;
	background:#2b529c;
}

#pager .selectedPage{
	color:#fff;
	background:#2b529c;
}

