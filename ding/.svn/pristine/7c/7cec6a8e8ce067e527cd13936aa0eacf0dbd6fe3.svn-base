<!DOCTYPE html>
<html lang="zh-cmn" xmlns="http://www.w3.org/1999/xhtml"
      xmlns:th="http://www.thymeleaf.org">
<head>
    <meta charset="utf-8">
    <title>选择车电</title>
    <header th:replace="comm :: checkManagerLogin"></header>
    <header th:replace="comm :: header_web"></header>
    <style type="text/css">
        a{text-decoration:underline;}
        .col-xs-4{padding:0px;}
        .col-xs-8{padding:0px;}
        .col-sm-4{padding:0px;}
        .col-sm-8{padding:0px;padding-right:10px;}
        .a-reason{background-color: red;text-decoration:underline;}

    </style>
</head>
<body class="gray-bg">



<div class="wrapper wrapper-content animated fadeInRight">

    <div class="row">
        <div class="col-sm-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <form role="form" class="form-inline" id="searchForm">

                        <div class="form-group">
                            <label for="searchbrandId">品牌</label>
                            <select name="selectbrand" id="searchbrandId" class="form-control">
                                <option value="">全部</option>
                            </select>
                        </div>
                        <div class="form-group">
                            <label for="searchmodelType">类型</label>
                            <select type="text" placeholder="" id="searchmodelType" class="form-control keyupclick"  style="width:120px;">
                                <option value="">全部</option>
                                <option value="0">车辆</option>
                                <option value="1">电池</option>
                            </select>
                        </div>
                        <div class="form-group">
                            <label for="searchvehicleId">编号</label>
                            <input type="text" placeholder="" id="searchvehicleId" class="form-control keyupclick"  style="width:120px;">
                        </div>
                        <button type="button" class="btn btn-outline btn-default" onclick="loadData(1)">
                            <i class="glyphicon glyphicon-search" aria- hidden="true"></i>查询
                        </button>
                        <button type="button" class="btn btn-outline btn-warning" onclick="clearSearch()">
                            <i class="glyphicon" aria-hidden="true"></i>清除查询条件
                        </button>
                    </form>

                </div>

                <div class="ibox-content">

                    <div class="btn-group" id="exampleToolbars" role="group">
                        <button type="button" class="btn btn-outline btn-default" onclick="refreshData()">
                            <i class="glyphicon glyphicon-refresh" aria- hidden="true"></i>刷新
                        </button>
                        <button type="button" class="btn btn-outline btn-default" onclick="vehicleOutCom()">
                            确认出库
                        </button>
                        <button type="button" class="btn btn-outline btn-default" onclick="">
                            导出
                        </button>

                    </div>
                    <input type="hidden" th:value="${#httpServletRequest.getParameter('branchId')}" id="branchId">
                    <div>
                        <h5>公司ID:<span th:text="${#httpServletRequest.getParameter('branchId')}"></span>&nbsp;&nbsp;&nbsp;公司名称:<span th:text="${#httpServletRequest.getParameter('branchName')}"></span></h5>
                    </div>
                    <div  style="position:relative;width: 100%;overflow-x:
scroll;border:1px solid #eee;margin-bottom:30px;">
                        <table class="tbsalelist table table-striped table-bordered" data-toggle="table"  data-click-to-select="true" data-select-item- name="myRadioName" data-mobile-responsive="true">
                            <thead>
                            <tr>
                                <th width="40" id="title-table-checkbox">全选
                                </th>
                                <th width="60">ID</th>
                                <th width="80">品牌</th>
                                <th width="80">编码</th>
                                <th width="80">型号</th>
                                <th width="80">类型</th>
                                <th width="80">车架号</th>
                                <th width="80">GPS号</th>
                                <th width="80">操作</th>
                            </tr>
                            </thead>
                            <tbody id="table_content">
                            </tbody>
                        </table>
                    </div>
                    <div class="btn-group" id="exampleToolbar" role="group">
                        <button type="button" class="btn btn-outline btn-default" onclick="refreshData()">
                            <i class="glyphicon glyphicon-refresh" aria-hidden="true"></i>刷新
                        </button>
                        <button type="button" class="btn btn-outline btn-default" onclick="vehicleOutCom()">
                            确认出库
                        </button>
                        <button type="button" class="btn btn-outline btn-default" onclick="">
                            导出
                        </button>

                    </div>
                    <div class="btn-group" role="group" style="width:100%;">
                        <header th:replace="comm :: pagenation_web"></header>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<footer th:replace="comm :: footer_web"></footer>
</body>
</html>
<script type="text/javascript">
    var index ='';
    var rowData = [];
    var roleArray = [ "管理员", "普通用户" ];
    $(function() {
        //$(".tbsalelist").width(2200);
        //$(".tbsalelist").css("max-width", 2200);
        //initUpload('uploadBtn','showImg',null,1024);

        loadData(1);
        selectbrand();
        $('.keyupclick').on('keypress', function(evt){
            if (evt.keyCode == 13) {
                loadData(1);
            }

        });

    });
    function clearSearch(){
        $('#searchForm input').val('');
        $('#searchForm select').val('');
        $('#selectSalerSearch').val('');
        $('#selectKefuSearch').val('');
    }

    function showSystemLog(msgType,userId){
        parent.locationCurrMenu();
        parent.addPage("web登陆日志","${ctx}/pages/system/systemlog.jsp?msgType="+msgType+"&userId="+userId);

    }
    function refreshData(){
        loadData(pageinfo["page"]);
    }

    function loadData(page, size) {
        if (size != undefined) {
            pageinfo["size"] = size;
            $('#pagesize').html(size);
        }
        if (page == -1)
            pageinfo["page"]--;
        else if (page == -2)
            pageinfo["page"]++;
        else {
            if (page >= pageinfo["tp"] && pageinfo["tp"] > 0)
                pageinfo["page"] = pageinfo["tp"];
            else
                pageinfo["page"] = page;
        }
        pageinfo["brandId"]=$('#searchbrandId').val();
        pageinfo["modelType"]=$('#searchmodelType').val();
        pageinfo["vehicleId"]=$('#searchvehicleId').val();

        $('#table_content').html('<tr><td colspan="23">'+$('#divLoading').html()+'</td></tr>');
        httpPost(ctx+'/vehicle/noOutTbVehicleList',
            pageinfo,
            function(msg, status) {
                if (msg.status == 200) {
                    rowData = msg.rows;
                    var array = [];
                    for ( var i = 0; i < msg.rows.length; i++) {
                        var data = msg.rows[i];
                        array.push('<tr>');

                        array.push('<td><input  type="checkbox" class="i-checks" name="checkbox-tr" value="' + data["vehicleId"]+ '"/></td>');
                        array.push('<td >'+data["vehicleId"]+'</td>');
                        array.push('<td >'+data["modelNumber"].tbBrand.brandName+'</td>');
                        array.push('<td >'+data["serialNumber"]+'</td>');
                        array.push('<td >'+data["modelNumber"].model+'</td>');
                        if (data["type"] == 0) {
                            array.push('<td>车辆</td>');
                        } else if (data["type"] == 1) {
                            array.push('<td>电池</td>');
                        }else{
                            array.push('<td>数据不匹配</td>');
                        }
                        array.push('<td >'+data["vin"]+'</td>');
                        array.push('<td >'+data["gpsNumber"]+'</td>');
                        array.push('<td >'+'<a href="#">打印</a>'+'</td>');
                        array.push('</tr>');
                    }
                    $('#table_content').html(array.join(''));
                    initpageInfo(msg.total);
                    array=[];

                } else {
                    alert(msg.msg);

                }
            });
    }
    function selectbrand() {
        httpPost(ctx+'/brand/findList',
            pageinfo,
            function(msg, status) {
                if (msg.status == 200) {
                    rowData = msg.rows;
                    var array = [];
                    for ( var i = 0; i < msg.rows.length; i++) {
                        var data = msg.rows[i];
                        var option='<option value="'+data.brandId+'">'+data.brandName+'</option>';
                        $("[name=selectbrand]").append(option);
                    }
                } else {
                    alert(msg.msg);
                }
            });
    }
    //出库到分公司
    function vehicleOutCom() {
        var bar=document.getElementById("branchId").value;
        var delArr = [];
        $('input[name="checkbox-tr"]').each(function(index, data) {
            if($(this).parent().attr('class').indexOf('checked')!=-1){
                delArr.push($(this).val());
            }
        });
        if(delArr.length==0){
            swal({
                title:'信息提示',
                text:'请至少选择一项待出库！',
                type:'warning',
                confirmButtonText:'确定'
            });
            return;
        }
        swal({
            title: "确认出库所选车辆吗？",
            text: "",
            type: "warning",
            showCancelButton: true,
            confirmButtonColor: "#DD6B55",
            confirmButtonText: "确定",
            cancelButtonText: "取消",
            closeOnConfirm: true
        },function(){

            var params="delArr="+delArr+"&branchId="+bar;
            $.post(ctx+"/vehicle/vehicleOutCom",params,function(data){
                if(data["status"]==200){
                    swal({
                        title:"信息提示",
                        text:"成功",
                        type:"success",
                        confirmButtonText:'确定'
                    })
                    loadData(pageinfo["page"]);
                }else {
                    swal({
                        title:"信息提示",
                        text:"操作失败",
                        type:"error",
                        confirmButtonText:'确定'
                    })
                }
            });

        })
    }


</script>


