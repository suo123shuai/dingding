    <select id="findUser" resultMap="BaseResultMap" parameterType="java.lang.Long" >
        select
        <include refid="Base_Column_List" />
        from tb_user
        where user_id = #{userId,jdbcType=BIGINT} and type = 3
        and delete_flag =0
    </select>

    <select id="findUserTuan" parameterType="long" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from tb_user
        where user_id = #{userId,jdbcType=BIGINT}
    </select>

    <select id="findUserState" resultType="com.ddcar.entity.TbUser" parameterType="Map" >
        select
        <include refid="Base_Column_List" />
        from tb_user
        where user_id in
        (SELECT DISTINCT user_id FROM tb_vehicle WHERE
        wang_user_id=#{wangUserId} AND manager=6 AND state=1 and delete_flag =0)
        and type = 3 and delete_flag = 0
        <if test="fuzzyField != null">
            and (user_name LIKE "%"#{fuzzyField}"%"
            OR phone LIKE "%"#{fuzzyField}"%")
        </if>
    </select>

    <select id="listUsetVehicleGet1" resultType="com.ddcar.entity.Vehicles">
  SELECT v.start_rental_time,v.stop_rental_time,v.serial_number,v.vehicle_id,v.wang_user_id,
  v.monthly_rent, mn.model,v.type, v.deposit cashDeposit,v.stop_rental_time,v.branch_id
  FROM tb_vehicle v join tb_model_number mn on v.model_id = mn.model_id
  and v.user_id = #{userId} and v.state = 1 and v.delete_flag = 0
  </select>

    <select id="listUsetVehicleGet" resultType="com.ddcar.entity.Vehicles" parameterType="Map">
        SELECT v.start_rental_time,v.stop_rental_time,v.serial_number,v.vehicle_id,v.wang_user_id,
        v.monthly_rent, mn.model,v.type, v.deposit cashDeposit,v.stop_rental_time,v.branch_id
        FROM tb_vehicle v join tb_model_number mn on v.model_id = mn.model_id
        and v.user_id = #{userId} and v.state = 1 and v.delete_flag = 0 and v.vehicle_id in
        <foreach collection="ids" index="index" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>

