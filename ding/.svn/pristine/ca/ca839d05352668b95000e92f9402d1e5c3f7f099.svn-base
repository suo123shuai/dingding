    <select id="getUserByUserId" parameterType="long" resultType="com.ddcar.entity.TbUser">
    select u.user_id, u.user_name, u.id_front, u.id_contrary, u.head_photo,
    u.id_number, u.phone, u.state, u.cash_deposit,u.true_name,
    u.balance, u.type, u.parent_id, u.use_money1, u.address,
    u.qr_code, u.account, u.password, u.weixin_id,b.branch_name,
